public class teste {

	public static void main(String[] args) {
		FuncionarioTeste teste = new FuncionarioTeste();
		teste.setSalario(3000);
		System.out.println(teste.getTipo());
		System.out.println(teste.getBonificacao());
		
		FuncionarioTeste f2 = new FuncionarioTeste();
		f2.setTipo(1);
		f2.setSalario(3000);
		System.out.println(f2.getTipo());
		System.out.println(f2.getBonificacao());
		

	}

}
